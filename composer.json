{
  "name": "bigbank/gcm-message",
  "type": "library",
  "description": "A PHP library to send messages to devices registered through Google Cloud Messaging",
  "keywords": [
    "GCM",
    "push message",
    "android",
    "iphone"
  ],
  "authors": [
    {
      "name": "Aivar Somelar",
      "email": "aivarsomelar@gmail.com",
      "role": "Developer"
    }
  ],
  "license": "Apache-2.0",
  "require": {
    "php": ">=5.6",
    "guzzlehttp/guzzle": "~6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4"
  },
  "minimum-stability": "RC",
  "autoload": {
    "psr-4": {
      "Bigbank\\Gcm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bigbank\\Gcm\\Test\\": "tests/"
    }
  }
}
